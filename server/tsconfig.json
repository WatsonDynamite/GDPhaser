{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "nodenext",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowUnreachableCode": false,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "lib": ["dom", "es6", "esnext"]
  },
  "ts-node": {
    "esm": true
  }
}
